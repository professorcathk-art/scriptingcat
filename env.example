# OpenAI API Key for AI analysis and script generation
OPENAI_API_KEY=your_openai_api_key_here

# Instagram API Key (optional - for Instagram content extraction)
INSTAGRAM_API_KEY=your_instagram_api_key_here

# Database URL for subscription management
# For Vercel Postgres: postgres://username:password@host:port/database
# For other providers, use their connection string format
DATABASE_URL=your_database_url_here

# Next.js environment
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Stripe keys for payment processing
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
